<!doctype html>
<html lang="en" class="no-js">
<head>
<meta charset="utf-8">
<!--[if IE]><![endif]-->

<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"> <!-- render with chromeframe plugin on IE where available -->

<title><%= h(@page_title || application_name) %></title>

<meta name="description" content="Academic Commons is Columbia University's digital repository for academic research and scholarship produced by the university community.">

<!--  <meta name="author" content=""> -->

<meta name="viewport" content="width=device-width; initial-scale=1.0; maximum-scale=1.0;">

<!--  <link rel="shortcut icon" href="/favicon.ico"> -->

<!--  <link rel="apple-touch-icon" href="/apple-touch-icon.png"> -->
    
  
<link href="<%= url_for :controller=>'catalog',:action => 'opensearch', :format => 'xml', :only_path => false %>" title="<%= application_name%>" type="application/opensearchdescription+xml" rel="search"/>
    <%#= stylesheet_link_tag 'compiled/screen.css', :media => 'screen, projection' %>
    <%#= stylesheet_link_tag 'compiled/print.css', :media => "print" %>
    <!-- [if lt IE8] <%= stylesheet_link_tag "compiled/ie.css", :media => "screen, projection" %> -->

    <%= render_head_content %>
    
 <link href="/stylesheets/blueprint/screen.css" media="screen, projection" rel="stylesheet" type="text/css" />
<link href="/stylesheets/application.css" media="screen, projection" rel="stylesheet" type="text/css" />
<%#= stylesheet_link_tag "blueprint/screen.css", :media => "screen, projection" %>
<%#= stylesheet_link_tag "application.css", :media => "screen, projection" %>
        
    <% javascript_tag do -%>
    <%= yield(:jstemplates) %> 
    <% end -%>
  
</head>


<% if params[:q].to_s.empty? and params[:f].to_s.empty? and params[:id].nil? %>
<!--[if lt IE 7 ]> <body class="ie6" onload="$('input#q').focus();"> <![endif]-->
<!--[if IE 7 ]>    <body class="ie7" onload="$('input#q').focus();"> <![endif]-->
<!--[if IE 8 ]>    <body class="ie8" onload="$('input#q').focus();"> <![endif]-->
<!--[if IE 9 ]>    <body class="ie9" onload="$('input#q').focus();"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <body onload="$('input#q').focus();"> <!--<![endif]--> 
<% else %>
<!--[if lt IE 7 ]> <body class="ie6"> <![endif]-->
<!--[if IE 7 ]>    <body class="ie7"> <![endif]-->
<!--[if IE 8 ]>    <body class="ie8"> <![endif]-->
<!--[if IE 9 ]>    <body class="ie9"> <![endif]-->
<!--[if (gt IE 9)|!(IE)]><!--> <body> <!--<![endif]-->
<% end %>
  
  
<div  class="container">

<header class="span-24 last"> 
	<div id="rooftop">
		<img src="/images/crown.w18h14.white.gif" width="18" height="14" border="0">  <a href="http://www.columbia.edu" style="color:white; text-decoration:none" target="_blank">Columbia University Libraries</a>
	</div>
	<div id="hd">
		<div id="util_links">
			<%= render :partial=>'/user_util_links' %>
		</div>
	
		<br style="clear:both">
	
		<img src="/images/cu_logo.png" id="cuLogo" alt="Columbia University Logo"> 
	
		<h1 class="site_title"><%= link_to application_name, root_path %><%= (params[:controller] == "catalog" and !params[:id]) ? "<span id='results_text'>Search Results</span>" : "" %></h1>
	
	</div> <!-- /hd -->

</header>

	<div id="srch" class="span-24 last">
	    <%= render :partial=>'/catalog/search_form' %>
	    <%= render :partial=>'/catalog/constraints' %>
	</div>
    
<div id="page" class="span-24 clearfix">
    
    <div id="sidebar" class="span-6">
    	<div id="facet-wrapper">
			<%= sidebar_items.join('') %>
		</div>
	</div>
	
	<div id="content" class="span-18 last">
		<%= render :partial=>'/flash_msg' %>
		<%= yield %>
	</div>
          
</div><!-- div#page -->

<footer class="span-24 last">
	<div id="footer"> 
	
		<%= @footer %>
	
	</div>
</footer> 

</div>  <!-- div#container -->    

</body>
  
</html>