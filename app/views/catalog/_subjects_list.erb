<h1 class="content-title">Academic Commons Content by Subject</h1>


<div  id='browse-box' class='span-22 prepend-2'>  
<% get_subjects_list.in_groups(3, fill_with=false) do |group| %>
   <div class='span-7 browse-list'>
    <ul>
 
    <% group.each do |v| %>
      <li> <%= link_to(v[:name], search_catalog_path()+'?f%5Bsubject_facet%5D%5B%5D=' + url_encode_resource(v[:name]) ) %> (<%= v[:count] %>)  </li>
    <% end %>
    </ul>
    </div>
 
<% end %>

</div>

