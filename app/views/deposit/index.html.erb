<div id="static-text">

  <div class="page-title">
    <div class="container">
      <h1>Upload Your Research</h1>
    </div>
  </div>

	<div class="upload-landing">
			<div class="log-in">
				<h3>Have a Columbia UNI?</h3>
				<p>Log in with your Columbia UNI and password</p>
				<%= link_to t('blacklight.header_links.login'), new_user_session_path, class: 'btn btn-primary' %>
			</div>
			<div class="no-log-in">
				<h3>No Columbia UNI?</h3>
				<p>Tell us about your work and how it meets our <%= link_to 'guidelines for participation', policies_path %>.</p>
				<p>Email <%= mail_to "ac@columbia.edu" %></p>
			</div>
  </div>

</div>
